{
  "$schema": "../node_modules/wrangler/config-schema.json",
  "name": "objects-test",
  "main": "./objects-test-worker.ts",
  "compatibility_date": "2025-09-06",
  "compatibility_flags": ["nodejs_compat"],

  // Durable Objects - DOBase, Entity, Worker, Collection, Agent, Human, Workflow with SQLite storage
  "durable_objects": {
    "bindings": [
      {
        "name": "DO_BASE",
        "class_name": "TestDOBase"
      },
      {
        "name": "ENTITY",
        "class_name": "TestEntity"
      },
      {
        "name": "WORKER",
        "class_name": "TestWorker"
      },
      {
        "name": "COLLECTION",
        "class_name": "TestCollection"
      },
      {
        "name": "AGENT",
        "class_name": "TestAgent"
      },
      {
        "name": "HUMAN",
        "class_name": "TestHuman"
      },
      {
        "name": "WORKFLOW",
        "class_name": "TestWorkflow"
      }
    ]
  },

  // Migrations - Enable SQLite for all DO classes
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["TestDOBase", "TestEntity", "TestWorker"]
    },
    {
      "tag": "v2",
      "new_sqlite_classes": ["TestCollection", "TestAgent", "TestHuman", "TestWorkflow"]
    }
  ]
}
