{
  "$schema": "../node_modules/wrangler/config-schema.json",
  "name": "do-with-integration-test",
  "main": "./do-with-test-worker.ts",
  "compatibility_date": "2025-09-06",
  "compatibility_flags": ["nodejs_compat"],

  // Durable Objects - multiple DO classes for testing different feature configs
  "durable_objects": {
    "bindings": [
      {
        "name": "DO_WITH_THINGS",
        "class_name": "DOWithThings"
      },
      {
        "name": "DO_WITH_SEARCH",
        "class_name": "DOWithSearch"
      },
      {
        "name": "DO_WITH_MULTIPLE",
        "class_name": "DOWithMultiple"
      },
      {
        "name": "DO_BASE",
        "class_name": "DOBase"
      }
    ]
  },

  // Migrations - Enable SQLite for all test DO classes
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["DOWithThings", "DOWithSearch", "DOWithMultiple", "DOBase"]
    }
  ]
}
