{
  "name": "@dotdo/sqs",
  "version": "0.1.0",
  "description": "AWS SQS SDK compat layer for Cloudflare Workers with DO backing",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "keywords": [
    "sqs",
    "aws",
    "queue",
    "messaging",
    "cloudflare",
    "workers",
    "durable-objects",
    "serverless"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dotdo/dotdo",
    "directory": "compat/sqs"
  },
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250109.0",
    "typescript": "^5.0.0",
    "tsup": "^8.0.0"
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  }
}
