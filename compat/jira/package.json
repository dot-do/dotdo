{
  "name": "@dotdo/jira",
  "version": "0.1.0",
  "description": "Jira API compatible client for Cloudflare Workers",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./issues": {
      "types": "./dist/issues.d.ts",
      "import": "./dist/issues.js"
    },
    "./projects": {
      "types": "./dist/projects.d.ts",
      "import": "./dist/projects.js"
    },
    "./webhooks": {
      "types": "./dist/webhooks.d.ts",
      "import": "./dist/webhooks.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "jira",
    "atlassian",
    "issue-tracker",
    "project-management",
    "cloudflare-workers",
    "edge",
    "sdk",
    "compat"
  ],
  "author": "dotdo",
  "license": "MIT",
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
