  "repository": {
    "type": "git",
    "url": "https://github.com/dotdo/dotdo",
    "directory": "compat/stripe"
  },
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250109.0",
    "typescript": "^5.0.0",
    "tsup": "^8.0.0"
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
