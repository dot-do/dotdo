{
  "name": "@dotdo/stripe",
  "version": "0.1.0",
  "description": "Stripe API compat layer for Cloudflare Workers with edge compatibility",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
