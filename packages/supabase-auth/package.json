{
  "name": "@dotdo/supabase-auth",
  "version": "0.1.0",
  "description": "Supabase Auth SDK with in-memory backend for testing and edge environments",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "supabase",
    "auth",
    "authentication",
    "oauth",
    "jwt",
    "cloudflare",
    "workers",
    "edge",
    "serverless",
    "in-memory",
    "testing"
  ],
  "author": "dotdo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotdo/dotdo",
    "directory": "packages/supabase-auth"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  }
}
