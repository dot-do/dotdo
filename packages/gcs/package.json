{
  "name": "@dotdo/gcs",
  "version": "0.1.0",
  "description": "Google Cloud Storage compatible client with pluggable storage backends",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./backend": {
      "types": "./src/backend.ts",
      "import": "./src/backend.ts",
      "default": "./src/backend.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "gcs",
    "google-cloud-storage",
    "storage",
    "object-storage",
    "r2",
    "cloudflare-workers",
    "durable-objects"
  ],
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
