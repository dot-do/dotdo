{
  "name": "@dotdo/google-ai",
  "version": "0.1.0",
  "description": "Google Generative AI compatibility layer for edge environments - drop-in replacement for @google/generative-ai",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "google-ai",
    "gemini",
    "generative-ai",
    "llm",
    "ai",
    "embeddings",
    "edge",
    "cloudflare-workers",
    "compatibility"
  ],
  "author": "dotdo",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  }
}
