{
  "name": "@dotdo/worker-types",
  "version": "0.1.0",
  "type": "module",
  "description": "Shared TypeScript interfaces and type guards for dotdo workers, agents, and humans",
  "author": "dotdo <hello@dotdo.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotdo-dev/dotdo.git",
    "directory": "packages/worker-types"
  },
  "homepage": "https://github.com/dotdo-dev/dotdo/tree/main/packages/worker-types#readme",
  "bugs": {
    "url": "https://github.com/dotdo-dev/dotdo/issues"
  },
  "keywords": [
    "dotdo",
    "workers",
    "agents",
    "humans",
    "types",
    "typescript",
    "interfaces",
    "cloudflare",
    "durable-objects"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./worker": {
      "types": "./dist/worker.d.ts",
      "import": "./dist/worker.js",
      "require": "./dist/worker.cjs"
    },
    "./agent": {
      "types": "./dist/agent.d.ts",
      "import": "./dist/agent.js",
      "require": "./dist/agent.cjs"
    },
    "./human": {
      "types": "./dist/human.d.ts",
      "import": "./dist/human.js",
      "require": "./dist/human.cjs"
    },
    "./guards": {
      "types": "./dist/guards.d.ts",
      "import": "./dist/guards.js",
      "require": "./dist/guards.cjs"
    },
    "./routing": {
      "types": "./dist/routing.d.ts",
      "import": "./dist/routing.js",
      "require": "./dist/routing.cjs"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  },
  "sideEffects": false
}
