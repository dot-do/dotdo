{
  "name": "@dotdo/openai",
  "version": "0.1.0",
  "type": "module",
  "description": "OpenAI API compatibility layer with edge support",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "openai",
    "gpt",
    "llm",
    "ai",
    "cloudflare-workers",
    "edge",
    "chat",
    "embeddings"
  ],
  "author": "dotdo",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  }
}
