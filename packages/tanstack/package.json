{
  "name": "@dotdo/tanstack",
  "version": "0.1.0",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "import": "./src/server.ts",
      "default": "./src/server.ts"
    },
    "./client": {
      "types": "./src/client.ts",
      "import": "./src/client.ts",
      "default": "./src/client.ts"
    },
    "./react": {
      "types": "./src/react.ts",
      "import": "./src/react.ts",
      "default": "./src/react.ts"
    },
    "./protocol": {
      "types": "./src/protocol.ts",
      "import": "./src/protocol.ts",
      "default": "./src/protocol.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@tanstack/db": "^0.5.0",
    "@tanstack/react-db": "^0.1.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-db": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  }
}
