{
  "name": "@dotdo/sentry",
  "version": "0.1.0",
  "description": "Sentry SDK compatibility layer for edge environments with in-memory backend support",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./breadcrumbs": {
      "types": "./src/breadcrumbs.ts",
      "import": "./src/breadcrumbs.ts",
      "default": "./src/breadcrumbs.ts"
    },
    "./capture": {
      "types": "./src/capture.ts",
      "import": "./src/capture.ts",
      "default": "./src/capture.ts"
    },
    "./transport": {
      "types": "./src/transport.ts",
      "import": "./src/transport.ts",
      "default": "./src/transport.ts"
    },
    "./scope": {
      "types": "./src/scope.ts",
      "import": "./src/scope.ts",
      "default": "./src/scope.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "import": "./src/types.ts",
      "default": "./src/types.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "sentry",
    "error-tracking",
    "monitoring",
    "observability",
    "cloudflare",
    "workers",
    "edge",
    "serverless",
    "breadcrumbs",
    "error-reporting"
  ],
  "author": "dotdo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotdo/dotdo",
    "directory": "packages/sentry"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  }
}
