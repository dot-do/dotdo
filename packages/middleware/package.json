{
  "name": "@dotdo/middleware",
  "version": "0.1.0",
  "description": "Hono middleware for dotdo applications",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./auth": {
      "import": "./dist/auth/index.js",
      "types": "./dist/auth/index.d.ts"
    },
    "./auth/jwt": {
      "import": "./dist/auth/jwt.js",
      "types": "./dist/auth/jwt.d.ts"
    },
    "./auth/api-key": {
      "import": "./dist/auth/api-key.js",
      "types": "./dist/auth/api-key.d.ts"
    },
    "./auth/session": {
      "import": "./dist/auth/session.js",
      "types": "./dist/auth/session.d.ts"
    },
    "./workos": {
      "import": "./dist/workos/index.js",
      "types": "./dist/workos/index.d.ts"
    },
    "./workos/authkit": {
      "import": "./dist/workos/authkit.js",
      "types": "./dist/workos/authkit.d.ts"
    },
    "./workos/vault": {
      "import": "./dist/workos/vault.js",
      "types": "./dist/workos/vault.d.ts"
    },
    "./error": {
      "import": "./dist/error/index.js",
      "types": "./dist/error/index.d.ts"
    },
    "./request-id": {
      "import": "./dist/request-id.js",
      "types": "./dist/request-id.d.ts"
    },
    "./rate-limit": {
      "import": "./dist/rate-limit.js",
      "types": "./dist/rate-limit.d.ts"
    },
    "./mcp": {
      "import": "./dist/mcp.js",
      "types": "./dist/mcp.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "!**/*.test.ts",
    "!**/__tests__"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "hono": "^4.0.0",
    "jose": "^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "hono": "^4.6.0",
    "jose": "^6.1.3",
    "typescript": "^5.7.3",
    "vitest": "^3.0.2"
  },
  "keywords": [
    "hono",
    "middleware",
    "dotdo",
    "authentication",
    "error-handling",
    "rate-limit",
    "workos"
  ],
  "author": "dotdo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/dotdo.git",
    "directory": "packages/middleware"
  },
  "bugs": {
    "url": "https://github.com/dot-do/dotdo/issues"
  }
}
