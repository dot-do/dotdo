{
  "name": "@dotdo/calls",
  "version": "0.1.0",
  "description": "Voice and Video Calling Service with Twilio API compatibility and WebRTC signaling",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "import": "./src/types.ts",
      "default": "./src/types.ts"
    },
    "./twilio-compat": {
      "types": "./src/twilio-compat.ts",
      "import": "./src/twilio-compat.ts",
      "default": "./src/twilio-compat.ts"
    },
    "./webrtc-signaling": {
      "types": "./src/webrtc-signaling.ts",
      "import": "./src/webrtc-signaling.ts",
      "default": "./src/webrtc-signaling.ts"
    },
    "./CallDO": {
      "types": "./src/CallDO.ts",
      "import": "./src/CallDO.ts",
      "default": "./src/CallDO.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "calls",
    "voice",
    "video",
    "webrtc",
    "twilio",
    "signaling",
    "twiml",
    "cloudflare",
    "workers",
    "durable-objects",
    "edge",
    "serverless"
  ],
  "author": "dotdo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotdo/dotdo",
    "directory": "packages/calls"
  },
  "dependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  }
}
