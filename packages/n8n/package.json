{
  "name": "@dotdo/n8n",
  "version": "0.1.0",
  "description": "n8n-compatible workflow automation engine for edge environments with in-memory execution",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "n8n",
    "workflow",
    "automation",
    "edge",
    "cloudflare-workers",
    "compatibility",
    "durable-objects"
  ],
  "author": "dotdo",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  }
}
