{
  "name": "@dotdo/client",
  "version": "0.2.0",
  "type": "module",
  "description": "Cap'n Web RPC Client SDK for Durable Objects with automatic promise pipelining",
  "author": "dotdo <hello@dotdo.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotdo-dev/dotdo.git",
    "directory": "packages/client"
  },
  "homepage": "https://github.com/dotdo-dev/dotdo/tree/main/packages/client#readme",
  "bugs": {
    "url": "https://github.com/dotdo-dev/dotdo/issues"
  },
  "keywords": [
    "dotdo",
    "durable-objects",
    "cloudflare",
    "rpc",
    "websocket",
    "client",
    "capnweb",
    "promise-pipelining"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "capnweb": "^0.1.0"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  },
  "sideEffects": false
}
