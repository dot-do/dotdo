{
  "name": "@dotdo/redis",
  "version": "0.1.0",
  "description": "Redis-compatible in-memory data store with ioredis-compatible API",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./commands/*": {
      "types": "./src/commands/*.ts",
      "import": "./src/commands/*.ts",
      "default": "./src/commands/*.ts"
    },
    "./backends/*": {
      "types": "./src/backends/*.ts",
      "import": "./src/backends/*.ts",
      "default": "./src/backends/*.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "redis",
    "in-memory",
    "cache",
    "key-value",
    "ioredis",
    "compatible"
  ],
  "author": "dotdo",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  }
}
