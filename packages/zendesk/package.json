{
  "name": "@dotdo/zendesk",
  "version": "0.1.0",
  "type": "module",
  "description": "Zendesk SDK compatible layer for Cloudflare Workers - drop-in replacement for node-zendesk",
  "author": "dotdo <hello@dotdo.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotdo-dev/dotdo.git",
    "directory": "packages/zendesk"
  },
  "homepage": "https://github.com/dotdo-dev/dotdo/tree/main/packages/zendesk#readme",
  "bugs": {
    "url": "https://github.com/dotdo-dev/dotdo/issues"
  },
  "keywords": [
    "dotdo",
    "zendesk",
    "customer-support",
    "ticketing",
    "helpdesk",
    "cloudflare",
    "workers",
    "edge",
    "tickets",
    "users",
    "organizations"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    },
    "./local": {
      "types": "./dist/local.d.ts",
      "import": "./dist/local.js",
      "require": "./dist/local.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250124.0",
    "tsup": "^8.5.1",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@cloudflare/workers-types": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "@cloudflare/workers-types": {
      "optional": true
    }
  },
  "sideEffects": false
}
