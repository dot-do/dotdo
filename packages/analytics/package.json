{
  "name": "@dotdo/analytics",
  "version": "0.1.0",
  "description": "Analytics SDK compatibility layer supporting Mixpanel, Amplitude, and PostHog APIs",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./mixpanel": {
      "types": "./src/mixpanel.ts",
      "import": "./src/mixpanel.ts",
      "default": "./src/mixpanel.ts"
    },
    "./amplitude": {
      "types": "./src/amplitude.ts",
      "import": "./src/amplitude.ts",
      "default": "./src/amplitude.ts"
    },
    "./posthog": {
      "types": "./src/posthog.ts",
      "import": "./src/posthog.ts",
      "default": "./src/posthog.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "analytics",
    "mixpanel",
    "amplitude",
    "posthog",
    "tracking",
    "edge",
    "cloudflare-workers",
    "compatibility"
  ],
  "author": "dotdo",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  }
}
