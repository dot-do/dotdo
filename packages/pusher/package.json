{
  "name": "@dotdo/pusher",
  "version": "0.1.0",
  "description": "Pusher SDK compat layer with in-memory pub/sub for local development and edge-native performance",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "import": "./src/server.ts",
      "default": "./src/server.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "pusher",
    "realtime",
    "websocket",
    "pubsub",
    "cloudflare",
    "workers",
    "durable-objects",
    "channels",
    "presence"
  ],
  "author": "dotdo",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250109.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  }
}
