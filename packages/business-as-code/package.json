{
  "name": "@dotdo/business-as-code",
  "version": "0.1.0",
  "type": "module",
  "description": "Business-as-Code primitives for Organization, Business, Company, and Org types with schema.org.ai integration",
  "author": "dotdo <hello@dotdo.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotdo-dev/dotdo.git",
    "directory": "packages/business-as-code"
  },
  "homepage": "https://github.com/dotdo-dev/dotdo/tree/main/packages/business-as-code#readme",
  "bugs": {
    "url": "https://github.com/dotdo-dev/dotdo/issues"
  },
  "keywords": [
    "dotdo",
    "business",
    "organization",
    "company",
    "org",
    "goals",
    "okr",
    "schema.org.ai",
    "primitives"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.7.0",
    "vitest": "^3.2.4"
  },
  "sideEffects": false
}
