{
  "name": "@dotdo/rpc",
  "version": "0.1.0",
  "type": "module",
  "description": "Universal SDK wrapper that wraps any npm package/SDK in RPC with a single line",
  "author": "dotdo <hello@dotdo.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotdo-dev/dotdo.git",
    "directory": "packages/rpc"
  },
  "homepage": "https://github.com/dotdo-dev/dotdo/tree/main/packages/rpc#readme",
  "bugs": {
    "url": "https://github.com/dotdo-dev/dotdo/issues"
  },
  "keywords": [
    "dotdo",
    "rpc",
    "proxy",
    "sdk",
    "wrapper",
    "cloudflare",
    "workers",
    "edge"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  },
  "sideEffects": false
}
