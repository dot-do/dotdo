{
  "name": "@dotdo/sqs",
  "version": "0.1.0",
  "description": "AWS SQS SDK compat layer for Cloudflare Workers with in-memory backend",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./backends/memory": {
      "types": "./src/backends/memory.ts",
      "import": "./src/backends/memory.ts",
      "default": "./src/backends/memory.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "sqs",
    "aws",
    "queue",
    "messaging",
    "cloudflare",
    "workers",
    "durable-objects",
    "serverless",
    "edge"
  ],
  "author": "dotdo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotdo/dotdo",
    "directory": "packages/sqs"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  }
}
