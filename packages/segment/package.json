{
  "name": "@dotdo/segment",
  "version": "0.1.0",
  "type": "module",
  "description": "Segment SDK compatible layer for Cloudflare Workers - drop-in replacement for @segment/analytics-node",
  "author": "dotdo <hello@dotdo.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotdo-dev/dotdo.git",
    "directory": "packages/segment"
  },
  "homepage": "https://github.com/dotdo-dev/dotdo/tree/main/packages/segment#readme",
  "bugs": {
    "url": "https://github.com/dotdo-dev/dotdo/issues"
  },
  "keywords": [
    "dotdo",
    "segment",
    "analytics",
    "customer-data-platform",
    "cdp",
    "cloudflare",
    "workers",
    "edge",
    "analytics.do",
    "identify",
    "track",
    "page",
    "group",
    "alias"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./analytics": {
      "types": "./dist/analytics.d.ts",
      "import": "./dist/analytics.js",
      "require": "./dist/analytics.cjs"
    },
    "./backend": {
      "types": "./dist/backend.d.ts",
      "import": "./dist/backend.js",
      "require": "./dist/backend.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250124.0",
    "tsup": "^8.5.1",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@cloudflare/workers-types": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "@cloudflare/workers-types": {
      "optional": true
    }
  },
  "sideEffects": false
}
